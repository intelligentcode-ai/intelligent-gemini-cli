{
  "mcpServers": {
    "myCodeGenerator": {
      "command": "/usr/local/bin/my-code-gen-server",
      "args": ["--model", "gpt-4", "--temp", "0.7"],
      "env": {
        "API_KEY": "$OPENAI_API_KEY"
      },
      "cwd": "/opt/code-gen-server",
      "timeout": 60000,
      "trust": true
    },
    "localPromptProcessor": {
      "command": "./scripts/local_processor.py",
      "args": ["--verbose"],
      "cwd": "./",
      "timeout": 10000
    }
  }
}
